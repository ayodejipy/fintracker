{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest --run tests/utils tests/composables",
    "test:components": "vitest --run tests/components",
    "test:server": "vitest --run tests/server tests/models",
    "test:integration": "vitest --run tests/integration",
    "test:e2e": "vitest --run tests/e2e",
    "test:performance": "vitest --run tests/performance",
    "test:all": "tsx tests/run-all-tests.ts",
    "test:ci": "CI=true tsx tests/run-all-tests.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "eslint . --fix",
    "format:check": "eslint . --max-warnings 0",
    "type-check": "vue-tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:prod": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:studio:prod": "prisma studio --browser none",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "db:status": "prisma migrate status"
  },
  "dependencies": {
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/icon": "^1.5.5",
    "@nuxt/image": "^1.11.0",
    "@nuxt/scripts": "^0.11.13",
    "@nuxt/ui": "^3.3.4",
    "@pinia/nuxt": "^0.11.2",
    "@prisma/client": "^6.16.2",
    "@sidebase/nuxt-auth": "^1.0.2",
    "@unhead/vue": "^2.0.17",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^13.9.0",
    "@vueuse/nuxt": "^13.9.0",
    "apexcharts": "^5.3.5",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "eslint": "^9.36.0",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.0.2",
    "nuxt": "^4.1.2",
    "pdf-parse": "^2.2.16",
    "pinia": "^3.0.3",
    "prisma": "^6.16.2",
    "tailwindcss": "^4.1.13",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.21",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.5.1",
    "vue3-apexcharts": "^1.8.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/vite": "^4.1.13",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.5.2",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^18.0.1",
    "playwright": "^1.55.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.7"
  }
}
